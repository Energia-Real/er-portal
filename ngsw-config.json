{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/**/*.js",
          "/**/*.css",
          "/assets/pdfs/*.pdf"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-cache",
      "urls": [
       "https://**",
       "https://er-auth-app.azurewebsites.net/**",
       "https://er-portal-gateway.azurewebsites.net/**",
       "https://er-domain-backend-dev.azurewebsites.net/**"
       
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 30,
        "maxAge": "6h",
        "timeout": "10s"
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/assets/**",
    "!/favicon.ico"
  ]
}
